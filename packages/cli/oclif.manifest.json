{
  "commands": {
    "build": {
      "aliases": [],
      "args": {},
      "description": "Analyses your backend searching for routes and bakes it into the runtime.",
      "examples": [
        {
          "command": "<%= config.bin %> <%= command.id %> -c kita.config.js",
          "description": "Builds your backend with a custom config file."
        },
        {
          "command": "<%= config.bin %> <%= command.id %> -d",
          "description": "Fast checks your backend for errors without generating the runtime."
        }
      ],
      "flags": {
        "import-source": {
          "char": "s",
          "description": "Maps all imports directly to source files instead of the usual dist folder. Needs tsx/ts-node to work.",
          "name": "import-source",
          "allowNo": false,
          "type": "boolean"
        },
        "config": {
          "char": "c",
          "description": "Path to your kita.config.js file, if any.",
          "name": "config",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        },
        "debug": {
          "description": "Prints full resolved config to stdout.",
          "name": "debug",
          "allowNo": false,
          "type": "boolean"
        },
        "dry-run": {
          "char": "d",
          "description": "Skips generation process. Useful for testing your code.",
          "name": "dry-run",
          "allowNo": false,
          "type": "boolean"
        }
      },
      "hasDynamicHelp": false,
      "id": "build",
      "pluginAlias": "@kitajs/cli",
      "pluginName": "@kitajs/cli",
      "pluginType": "core",
      "strict": true,
      "enableJsonFlag": false,
      "isESM": false,
      "relativePath": ["dist", "commands", "build.js"]
    },
    "init": {
      "aliases": [],
      "args": {},
      "description": "Creates a basic kita.config.js",
      "examples": ["<%= config.bin %> <%= command.id %>"],
      "flags": {
        "root": {
          "char": "r",
          "description": "Custom root directory for your project.",
          "name": "root",
          "hasDynamicHelp": false,
          "multiple": false,
          "type": "option"
        }
      },
      "hasDynamicHelp": false,
      "id": "init",
      "pluginAlias": "@kitajs/cli",
      "pluginName": "@kitajs/cli",
      "pluginType": "core",
      "strict": true,
      "enableJsonFlag": false,
      "isESM": false,
      "relativePath": ["dist", "commands", "init.js"]
    }
  },
  "version": "1.1.1"
}
