{
  "name": "@kitajs/cli",
  "version": "1.1.19",
  "description": "Performant and type safe fastify router - Build fast end-to-end APIs with ZERO abtraction cost!",
  "homepage": "https://kita.js.org",
  "bugs": {
    "url": "https://github.com/kitajs/kitajs/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kitajs/kitajs.git",
    "directory": "packages/cli"
  },
  "funding": "https://github.com/sponsors/arthurfiorette",
  "license": "MIT",
  "author": "Arthur Fiorette <kita@arthur.place>",
  "main": "dist/index.js",
  "bin": {
    "kita": "bin/run"
  },
  "files": [
    "bin",
    "dist",
    "npm-shrinkwrap.json",
    "oclif.manifest.json"
  ],
  "scripts": {
    "build": "tsc",
    "postbuild": "oclif manifest && oclif readme",
    "prepublish": "oclif manifest",
    "start": "node --require @swc-node/register index.ts"
  },
  "dependencies": {
    "@kitajs/common": "workspace:^",
    "@kitajs/generator": "workspace:^",
    "@kitajs/parser": "workspace:^",
    "@oclif/core": "^3.9.2",
    "@oclif/plugin-autocomplete": "^2.3.10",
    "@oclif/plugin-help": "^6.0.5",
    "@oclif/plugin-not-found": "^3.0.2",
    "@oclif/plugin-warn-if-update-available": "^2.1.1",
    "chalk": "^4.1.2",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2"
  },
  "devDependencies": {
    "@swc-node/register": "^1.6.8",
    "@swc/helpers": "^0.5.3",
    "@types/node": "^20.8.9",
    "oclif": "^4.0.3"
  },
  "peerDependencies": {
    "typescript": "^5.2.2"
  },
  "oclif": {
    "bin": "kita",
    "dirname": "kita",
    "commands": "dist/commands",
    "topicSeparator": " ",
    "plugins": [
      "@oclif/plugin-autocomplete",
      "@oclif/plugin-help",
      "@oclif/plugin-not-found",
      "@oclif/plugin-warn-if-update-available"
    ]
  }
}
